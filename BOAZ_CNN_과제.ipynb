      "source": [
        "import numpy as np\n",
        "from tensorflow.keras.preprocessing import image\n",
        "\n",
        "# ÏÉà Ïù¥ÎØ∏ÏßÄÎ•º Î°úÎìúÌïòÍ≥† Î™®Îç∏ ÏòàÏ∏° ÏàòÌñâ\n",
        "def predict_new_image(image_path, model, class_names):\n",
        "    # Ïù¥ÎØ∏ÏßÄ Î°úÎìú Î∞è Ï†ÑÏ≤òÎ¶¨\n",
        "    img = image.load_img(image_path, target_size=(32, 32))  # CIFAR-10 ÌÅ¨Í∏∞Î°ú Î¶¨ÏÇ¨Ïù¥Ï¶à ÎπàÏπ∏Ï±ÑÏö∞Í∏∞\n",
        "    img_array = image.img_to_array(img)  # Ïù¥ÎØ∏ÏßÄÎ•º Î∞∞Ïó¥Î°ú Î≥ÄÌôò\n",
        "    img_array = np.expand_dims(img_array, axis=0)  # Î∞∞ÏπòÎ•º ÏúÑÌï¥ Ï∞®Ïõê Ï∂îÍ∞Ä\n",
        "    img_array = img_array / 255.0  # Ï†ïÍ∑úÌôî\n",
        "\n",
        "    # ÏòàÏ∏° ÏàòÌñâ\n",
        "    predictions = model.predict(img_array)\n",
        "    predicted_class = np.argmax(predictions)  # Í∞ÄÏû• ÎÜíÏùÄ ÌôïÎ•†Ïùò ÌÅ¥ÎûòÏä§ Ïù∏Îç±Ïä§\n",
        "    class_name = class_names[predicted_class]  # ÌÅ¥ÎûòÏä§ Ïù¥Î¶Ñ\n",
        "\n",
        "    # Í≤∞Í≥º Ï∂úÎ†•\n",
        "    plt.imshow(img)\n",
        "    plt.title(f\"Predicted: {class_name}\")\n",
        "    plt.axis('off')\n",
        "    plt.show()\n",
        "\n",
        "# ÏÇ¨Ïö© ÏòàÏãú\n",
        "image_path = \"me.jpeg\"\n",
        "predict_new_image(image_path, model, class_names)\n"
      ]
    },
